/**app.wxss**/

.container {
    width: 100%;
    height: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-between;
    box-sizing: border-box;
    background-color: #f2f2f2;
    margin-bottom: 100rpx;
}
.container > view {
    width: 100%;
}

.banner image {
    width: 100%;
    height: 300rpx;
}

.success {
    color: #1aad16;
}

.error {
    color: #E81911;
}